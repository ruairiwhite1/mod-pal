{
    "name": "discord-uno",
    "version": "1.2.1",
    "description": "Create and handle an advanced version of UNO! In your very own JavaScript Discord Bot!",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc -p ."
    },
    "keywords": [
        "UNO!",
        "uno",
        "discord-uno",
        "discorduno"
    ],
    "exports": {
        ".": [
            {
                "require": "./lib/index.js",
                "import": "./esm/index.mjs"
            },
            "./lib/index.js"
        ],
        "./esm": "./esm/index.mjs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Maxisthemoose/discord-uno.git"
    },
    "bugs": {
        "url": "https://github.com/Maxisthemoose/discord-uno/issues"
    },
    "homepage": "https://github.com/Maxisthemoose/discord-uno#readme",
    "author": "Maxisthemoose",
    "license": "Apache 2.0",
    "devDependencies": {
        "typescript": "^4.0.2"
    },
    "dependencies": {
        "@types/node": "^14.10.1",
        "@types/ws": "^7.2.6",
        "canvas": "^2.6.1",
        "discord.js": "^12.3.1"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    }
}
